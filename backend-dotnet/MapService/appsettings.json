{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DataContent": {
    "Path": "App_Data"
  },
  "Documents": {
    "Path": "App_Data\\documents"
  },
  "Media": {
    "Audio": {
      "Path": "App_Data\\upload",
      "AllowedExtentions": [ "mp3", "wav", "ogg" ]
    },
    "Image": {
      "Path": "App_Data\\upload",
      "AllowedExtentions": [ "jpg", "jpeg", "png" ]
    },
    "Video": {
      "Path": "App_Data\\upload",
      "AllowedExtentions": [ "mp4", "mov", "ogg" ]
    }
  },
  "Swagger": {
    "Path": "Swagger",
    "File": "swagger.json"
  }
}
