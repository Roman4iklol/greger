{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Media": {
    "Audio": {
      "Path": "App_Data\\Upload",
      "AllowedExtentions": [ "mp3", "wav", "ogg" ]
    },
    "Image": {
      "Path": "App_Data\\Upload",
      "AllowedExtentions": [ "jpg", "jepg", "png" ]
    },
    "Video": {
      "Path": "App_Data\\Upload",
      "AllowedExtentions": [ "mp4", "mov", "ogg" ]
    }
  }
}
