{
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs\\log.txt"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "DataContent": {
    "Path": "App_Data"
  },
  "Documents": {
    "Path": "App_Data\\documents"
  },
  "Media": {
    "Audio": {
      "Path": "App_Data\\upload",
      "AllowedExtentions": [ "mp3", "wav", "ogg" ]
    },
    "Image": {
      "Path": "App_Data\\upload",
      "AllowedExtentions": [ "jpg", "jpeg", "png" ]
    },
    "Video": {
      "Path": "App_Data\\upload",
      "AllowedExtentions": [ "mp4", "mov", "ogg" ]
    }
  },
  "Swagger": {
    "Path": "Swagger",
    "File": "swagger.json"
  },
  "Templates": {
    "Path": "App_Data\\templates",
    "Name": "map.template"
  },
  "Upload": {
    "Path": "App_Data\\upload"
  },
  "FmeProxy": {
    "FmeServerBaseUrl": "https://fmeserver.some.domain.com",
    "FmeServerUser": "someFmeUser",
    "FmeServerPassword": "aGreatFmeUserPassword"
  }
}
